<div class="content private">
    <section class="main-section">
        <ul class="private-menu-lst">
            <li class="private-menu-lst-i"><a class="private-menu-lst-i__lk" href="/private/orders">Заказы</a></li>
            <li class="private-menu-lst-i"><a class="private-menu-lst-i__lk" href="/private/edit">Личные данные</a></li>
            <li class="private-menu-lst-i"><a class="private-menu-lst-i__lk" href="/private/password">Изменить пароль</a></li>
            <li class="private-menu-lst-i active"><a class="private-menu-lst-i__lk" href="/private/enterprize">Enterprize</a></li>
        </ul>
        <a class="btn btn-exit" href="/logout">Выход</a>
    </section>
    <section class="ep">
        <ul class="ep-list">
            {{#coupons}}
                <li class="ep-list-i">
                    <a href="/private/enterprize/coupon/{{id}}">
                        <div class="ep-list-i__inn">
                            <img class="ep-list-i__fishka" src="{{backgroundUrl}}">
                            <span class="ep-list-i__lbl"><img src="{{imageUrl}}"></span>
                        </div>
                    </a>
                    <div class="ep-list-i__txt">
                        <span class="ep-list-i__tl"><span class="accented">{{discountAmount}} <span class="rubl">{{discountUnit}}</span></span> на {{category}}</span>
                        <span class="ep-list-i__desc">{{description}}</span>
                    </div>
                </li>
            {{/coupons}}
        </ul>
    </section>
</div>