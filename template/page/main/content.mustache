<div class="content content-main">
    <div class="js-slides-img slidesImg" data-value="{{&promoDataValue}}">
	    <div class="js-slides-img-list slidesImgList">
			<a class="js-ga-click js-slides-img-item slidesImg_item slidesImg_item-left" href="">
				<img class="js-slides-img-cont slidesImg_cont" src="" />
			</a>

			<a class="js-ga-click js-slides-img-item slidesImg_item slidesImg_item-center" href="">
				<img class="js-slides-img-cont slidesImg_cont" src="" />
			</a>

			<a class="js-ga-click js-slides-img-item slidesImg_item slidesImg_item-right" href="">
				<img class="js-slides-img-cont slidesImg_cont" src="" />
			</a>
		</div>
		<div class="js-slides-img-left slidesImgBtn slidesImgBtn-left"></div>
		<div class="js-slides-img-right slidesImgBtn slidesImgBtn-right"></div>
	</div>

    {{#mainMenu}}
        <ul class="menuCat clearfix">
        {{#elements}}
            <li class="menuCat_item">
                <a class="js-ga-click menuCat_link" href="{{url}}" data-ga-click="{{&dataGa}}">
                    <span class="menuCat_chartWrap">
                        <span class="menuCat_chart"{{#style}} style="{{style}}"{{/style}}>{{char}}</span>
                        {{#image}}<img src="{{image}}" />{{/image}}
                    </span>
                    <span class="menuCat_name">{{&name}}</span>
                </a>
            </li>
        {{/elements}}
        </ul>
    {{/mainMenu}}

    <ul class="bannersLine clearfix">
		<li class="bannersLine_item">
			<a href="/how_choose_product" class="js-ga-click bannersLine_link" data-ga-click="{&quot;m_main_promo_link&quot;:[&quot;send&quot;,&quot;event&quot;,&quot;m_main_promo_link&quot;,&quot;Как выбрать&quot;]}">
				<img src="/css/modules/main-page/img/how_choose_product.png" alt="Как выбрать" class="bannersLine_img" />
			</a>
		</li>

		<li class="bannersLine_item">
			<a href="/enterprize" class="js-ga-click bannersLine_link" data-ga-click="{&quot;m_main_promo_link&quot;:[&quot;send&quot;,&quot;event&quot;,&quot;m_main_promo_link&quot;,&quot;Enterprize&quot;]}">
				<img src="/css/modules/main-page/img/enterprize.png" alt="Enter Prize" class="bannersLine_img" />
			</a>
		</li>

		<li class="bannersLine_item">
			<a href="/special_offers" class="js-ga-click bannersLine_link" data-ga-click="{&quot;m_main_promo_link&quot;:[&quot;send&quot;,&quot;event&quot;,&quot;m_main_promo_link&quot;,&quot;WOW&quot;]}">
				<img src="/css/modules/main-page/img/special_offers.png" alt="WOW акции" class="bannersLine_img" />
			</a>
		</li>

		<li class="bannersLine_item">
			<a href="/take_credit" class="js-ga-click bannersLine_link" data-ga-click="{&quot;m_main_promo_link&quot;:[&quot;send&quot;,&quot;event&quot;,&quot;m_main_promo_link&quot;,&quot;Кредит&quot;]}">
				<img src="/css/modules/main-page/img/take_credit.png" alt="Купи в кредит" class="bannersLine_img" />
			</a>
		</li>
    </ul>
</div>
