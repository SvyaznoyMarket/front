<div class="order-popup clearfix">
    <div class="order-popup__head">
        <span class="popup-close"><i class="icon-close"></i></span>
    </div>
    <div class="order-popup__top-block search">
        <form class="search__section">
            <div class="search-control {{! suggest-open }}">
                <i class="sprite-icon sprite-icon--search"></i>

                <input class="search-control__input" type="text" placeholder="Искать по улице, метро">
                <input class="search-control__submit" type="submit" value="">
                <div class="form-control-group__suggest suggest-control">
                    <ul class="suggest-control-lst">
                        <li class="suggest-control-lst-i">Первый Нижний Мойдодыровский проезд</li>
                        <li class="suggest-control-lst-i">Площадь Умывальников Начальника</li>
                        <li class="suggest-control-lst-i">Улица Мочалок Командира</li>
                        <li class="suggest-control-lst-i">Первый Нижний Мойдодыровский проезд</li>
                        <li class="suggest-control-lst-i">Площадь Умывальников Начальника</li>
                        <li class="suggest-control-lst-i">Улица Мочалок Командира</li>
                    </ul>
                </div>
            </div>
            <div class="btn btn--white btn--medium"><span class="map-link">Карта</span></div>
        </form>
    </div>

    <div class="custom-select__kit custom-filter__kit">
        <div class="custom-select custom-filter">
            <span class="custom-select__value" data-btn="select-point-type">
                <span class="bb-dotted">Все точки</span>
                <i class="caret"></i>
            </span>

            <ul class="custom-select-list" data-box="data-select-point-type">
                <li class="custom-select-list__item">
                    <input type="checkbox" id="shop1" name="paynow" class="custom-input custom-input--square-check" checked>

                    <label class="custom-label flex-hor-vertical-between" for="shop1"></label>
                    <span class="custom-label_tx">Магазины Enter.ru</span>
                </li>
                <li class="custom-select-list__item">
                    <input type="checkbox" id="shop2" name="paynow" class="custom-input custom-input--square-check" >

                    <label class="custom-label flex-hor-vertical-between" for="shop2"></label>
                    <span class="custom-label_tx">Постаматы Pick-Point</span>
                </li>
                <li class="custom-select-list__item">
                    <input type="checkbox" id="shop3" name="paynow" class="custom-input custom-input--square-check" >

                    <label class="custom-label flex-hor-vertical-between" for="shop3"></label>
                    <span class="custom-label_tx">Почта России</span>
                </li>
                <li class="custom-select-list__item">
                    <input type="checkbox" id="shop4" name="paynow" class="custom-input custom-input--square-check" >

                    <label class="custom-label flex-hor-vertical-between" for="shop4"></label>
                    <span class="custom-label_tx">HermesDhd</span>
                </li>
            </ul>
        </div>

        <div class="custom-select custom-filter">
            <span class="custom-select__value" data-btn="select-price">
                <span class="bb-dotted">Стоимость</span>
                <i class="caret"></i>
            </span>

            <ul class="custom-select-list" data-box="data-select-price">
                <li class="custom-select-list__item">
                    <input type="checkbox" id="price1" name="paynow" class="custom-input custom-input--square-check" >

                    <label class="custom-label flex-hor-vertical-between" for="price1"></label>
                    <span class="custom-label_tx">Бесплатно</span>
                </li>
                <li class="custom-select-list__item">
                    <input type="checkbox" id="price2" name="paynow" class="custom-input custom-input--square-check">

                    <label class="custom-label flex-hor-vertical-between" for="price2"></label>
                    <span class="custom-label_tx">100 <span class="rubl">p</span></span>
                </li>
            </ul>
        </div>

        <div class="custom-select custom-filter">
            <span class="custom-select__value" data-btn="select-date">
                <span class="bb-dotted">Дата</span>
                <i class="caret"></i>
            </span>

            <ul class="custom-select-list" data-box="data-select-date">
                <li class="custom-select-list__item">
                    <input type="checkbox" id="date1" name="paynow" class="custom-input custom-input--square-check">

                    <label class="custom-label flex-hor-vertical-between" for="date1"></label>
                    <span class="custom-label_tx">Сегодня</span>
                </li>
                <li class="custom-select-list__item">
                    <input type="checkbox" id="date2" name="paynow" class="custom-input custom-input--square-check" >

                    <label class="custom-label flex-hor-vertical-between" for="date2"></label>
                    <span class="custom-label_tx">Завтра</span>
                </li>
                <li class="custom-select-list__item">
                    <input type="checkbox" id="date3" name="paynow" class="custom-input custom-input--square-check">

                    <label class="custom-label flex-hor-vertical-between" for="date3"></label>
                    <span class="custom-label_tx">Послезавтра</span>
                </li>
                <li class="custom-select-list__item">
                    <input type="checkbox" id="date3" name="paynow" class="custom-input custom-input--square-check">

                    <label class="custom-label flex-hor-vertical-between" for="date3"></label>
                    <span class="custom-label_tx">с 23.05 по 24.06</span>
                </li>
            </ul>
        </div>
    </div>

    <div class="pick-point">
        <table class="pick-point-list">
            <tbody>
            {{#points}}
            <tr class="pick-point-list-i shop-snippet">
                <td class="shop-snippet-name">
                    <img src="http://www.enter.ru/images/deliv-logo/enter.png" alt="" class="shop-snippet-name__img">
                    <span class="shop-snippet-name__text">Магазин Enter</span>
                </td>

                <td class="shop-snippet-info">
                    {{#subway}}
                    <div class="shop-snippet-metro">
                        <span class="shop-snippet-metro__bullet" style="background: {{color}};"></span>
                        <span class="shop-snippet-metro__text">{{name}}</span>
                    </div>
                    {{/subway}}

                    <div class="shop-snippet-address">{{name}} <br/>{{regime}}</div>
                </td>

                <td class="deliv-info-snippet">
                    <span class="deliv-info-snippet__date">{{date}}</span>
                    <span class="deliv-info-snippet__price">
                        {{#cost}}{{cost}} <span class="rubl">p</span>{{/cost}}
                        {{^cost}}Бесплатно{{/cost}}
                    </span>
                </td>
            </tr>
            {{/points}}

